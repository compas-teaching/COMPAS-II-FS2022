                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.geometry import Translation
from compas.geometry import Vector
from compas.geometry import Point
from compas.geometry import Frame
from compas.geometry import Polyhedron
import Rhino.Geometry as rg

## Start assembly definition
from compas.datastructures import Assembly
from compas.datastructures import Part

assembly = Assembly("assembly-exercise")
assembly.attributes['brick_shape'] = shape

key = 0

for row in range(num_rows):
    
    x, y, z = 0, 0, (shape.height / 2)
    
    p = Point(*list(point))
    z = p.z
    print z
    p.z = row * (shape.height + tolerance_height)
    
    if row % 2 == 0:
        v = Vector(1, 0, 0)
        xaxis = Vector(0, 1, 0)
        yaxis = Vector(-1, 0, 0)
    else:
        v = Vector(0, 1, 0)
        xaxis = Vector(1, 0, 0)
        yaxis = Vector(0, 1, 0)
        
    pointA = p - v * (shape.depth/2 + offset)
    pointB = p + v * (shape.depth/2 + offset)
    frameA = Frame(pointA, xaxis, yaxis)
    frameB = Frame(pointB, xaxis, yaxis)
    
    for frame in [frameA, frameB]:
        frame = frame.transformed(Translation.from_vector([x, y, z]))
        polyhedron = Polyhedron(*shape.to_vertices_and_faces())
        element = Part(frame=frame, shape=polyhedron)
        assembly.add_part(element, key)
        key += 1


