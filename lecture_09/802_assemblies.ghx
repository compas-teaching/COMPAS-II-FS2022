                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
from compas.geometry import project_points_line

height = shape.zsize

ptS, ptA, ptB, ptE = points
line = ptS, ptE
ptAj, ptBj = project_points_line([ptA, ptB], line)
ptAj, ptBj = rg.Point3d(*ptAj), rg.Point3d(*ptBj)
stepA = (ptAj - ptA)/(num_rows - 1)
stepB = (ptBj - ptB)/(num_rows - 1)
up = rg.Vector3d(0, 0, height + tolerance_height)

curves = []
for i, r in enumerate(range(num_rows)):
    crv = rg.Curve.CreateControlPointCurve(points, 3)
    curves.append(crv)
    points = [p + up for p in points]
    points[1] += stepA
    points[2] += stepB
