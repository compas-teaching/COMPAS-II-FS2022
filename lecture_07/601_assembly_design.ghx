                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas.artists import Artist
from compas.datastructures import Assembly
from compas.datastructures import Part
from compas.geometry import Box
from compas.geometry import Polyhedron
from compas.geometry import Frame
from compas.geometry import Transformation
from compas.geometry import Translation
from compas.geometry import NurbsCurve
from compas_rhino.conversions import RhinoPoint

# Create Wall as Assembly instance
wall = Assembly("exercise-wall")

# Create wall curve
points = [RhinoPoint.from_guid(p).to_compas() for p in points]
curve = NurbsCurve.from_interpolation(points)

HGAP = 0.005
VGAP = 0.002
divisions = curve.divide_by_length(part_shape.width + HGAP)

# Create brick frames
frames = []
key = 0
for row in range(int(rows)):
    Trow = Translation.from_vector((0, 0, part_shape.height / 2. + (part_shape.height + VGAP) * row))

    for div, i in zip(divisions, range(len(divisions))):
        point = curve.point_at(div)
        xaxis = curve.tangent_at(div)
        yaxis = xaxis.cross((0, 0, 1))
        frame = Frame(point, xaxis, yaxis)

        # Create bricks as parts
        width, height, depth = part_shape.width, part_shape.height, part_shape.depth
        brick_type = 'full'
        if row % 2 == 1 and i % len(divisions) == 0:
            brick_type = 'half'
        if row % 2 == 0 and i % (len(divisions)) == len(divisions)-1:
            brick_type = 'half'

        if brick_type == 'half':
            width = width / 2.
            xoffset = width / 2.
        else:
            xoffset = width / 2. if row % 2 == 0 else 0

        shape = Box.from_width_height_depth(width, height, depth)
        shape = Polyhedron(*shape.to_vertices_and_faces())

        Tb = Translation.from_vector((xoffset, 0, 0))
        Tf = Transformation.from_frame(frame)
        frame = Frame.from_transformation(Trow * Tf * Tb)
        frames.append(frame)

        part = Part(frame=frame, shape=shape)
        wall.add_part(part, key=str(key))
        key += 1


frames = [Artist(f).draw() for f in frames]
curve = Artist(curve).draw()
bricks = [Artist(brick.geometry).draw() for brick in wall.parts()]

